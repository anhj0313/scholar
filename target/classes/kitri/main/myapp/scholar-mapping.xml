<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cal">	
	<select id="calendarall" resultType="cal">		
		select * from calendar
	</select>
	<select id="userall" resultType="user">
		select * from user_info
	</select>
	<select id="calendarmonth" resultType="cal" parameterType="String"> <!--config에서 설정  -->	
		select * from calendar where substr(cal_date,4,2) = #{month}
	</select>

</mapper>







